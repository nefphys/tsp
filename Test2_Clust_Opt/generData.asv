%% 生成的数据集
 % 1000 5000 10000  9组数据 
 
%% 螺旋形
Num = 2000;
r = linspace(0,2000,Num);%半径
a = 1000;%圆心横坐标
b = 1000;%圆心纵坐标
theta = 0:pi/Num/2):2*pi; %角度[0,2*pi] 
theta = theta(1:(end-1));
noise = rand(1,length(theta))*0.3;
x = a+r.*cos(theta);
y = b+r.*sin(theta) + noise;
scatter(x,y)
hold on
r = linspace(5,15,2001);%半径
a = 10;%圆心横坐标
b = 10;%圆心纵坐标
theta = 0:pi/1000:2*pi; %角度[0,2*pi] 
noise = rand(1,length(theta))*0.3;
x = a+r.*cos(theta);
y = b+r.*sin(theta) + noise;
scatter(x,y)



