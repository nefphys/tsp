%% 月牙型
Num = 1000;
cof = 5;
x1 = linspace(-3,3,Num)';
noise = rand(Num,1) * 0.3;
y1 = - x1 .^ 2 /cof + 4.5 + noise;
scatter(x1,y1)
hold on
